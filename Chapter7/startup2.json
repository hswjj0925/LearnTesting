[
  {
    "description":"模拟一个不带参数的get请求",
    "request":{
      "uri":"/getDemoWithoutParam",
      "method":"get"
    },
    "response":{
      "text":"这是一个不带参数的请求"
    }
  },
  {
    "description":"模拟一个带参数的get请求",
    "request":{
      "uri":"/getDemoWithParam",
      "method":"get",
      "queries":{
        "name":"wangjiajia",
        "age":"18"
      }
    },
      "response":{
        "text":"汪佳佳今年18岁了"
      }
  },
  {
    "description":"模拟一个post请求",
    "request":{
      "uri":"/demoPost",
      "method":"post"
    },
    "response":{
      "text":"这是一个post请求"
    }
  },
  {
    "description":"模拟一个带cookie的get请求",
    "request":{
      "uri":"/get/with/cookie",
      "method":"get",
      "cookies":{
        "login":"true"
      }
    },
      "response":{
        "text":"这是一个带cookie的get请求"
      }
  },
  {
    "description":"模拟一个带json参数的post的请求，含cookies",
    "request":{
      "uri":"/demo/with/post",
      "method":"post",
      "cookies":{
        "login":"true"
      },
      "headers":{
        "Content-Type":"Application/json"
      },
      "json":{
        "name":"wangjiajia",
        "age":"18"
      }
    },
    "response":{
      "status":200,
      "json":{
        "wangjiajia":"success",
        "status":"ok"
      }
    }
  },
  {
    "description":"模拟一个含有header的post的请求",
    "request":{
      "uri":"/demo/with/headers",
      "method":"post",
      "headers":{
        "Content-Type":"Application-json"
      },
      "json":{
        "name":"wangjiajia",
        "age":"18"
      }
    },
    "response":{
      "status":200,
      "json":{
        "wangjiajia":"success",
        "status":"ok"
      }
    }
  },
  {
    "description":"模拟重定向",
    "request":{
      "uri":"/redirect"
    },
    "redirectTo":"/getDemoWithoutParam"
  },
  {
    "description":"获得cookies信息",
    "request":{
      "uri":"/getCookies",
      "method":"get"
    },
    "response":{
      "text":"获得cookie成功！",
      "cookies":{
        "login":"true"
      }
    }

  }
]

